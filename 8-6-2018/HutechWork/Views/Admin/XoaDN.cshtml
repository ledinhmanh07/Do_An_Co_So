@model HutechWork.Models.TAIKHOAN_DN

@{
    ViewBag.Title = "XoaDN";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Xác Nhận Xóa Tài Khoản Doanh Nghiệp</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Tài Khoản
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MATKDN)
        </dd>
        <dt>
            Tên Đăng Nhập
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ID)
        </dd>

        <dt>
            Mật Khẩu
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PASS)
        </dd>
        <dt>
            Logo
        </dt>

        <dd>
            <img src="~/img/@Model.TTDOANHNGHIEP.LOGO" />
        </dd>

        <dt>
            Tên Doanh Nghiệp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TTDOANHNGHIEP.TENDN)
        </dd>

        <dt>
            Quy Mô Doanh Nghiệp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TTDOANHNGHIEP.QUYMODN)
        </dd>

        <dt>
            Thông Tin Doanh Nghiệp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TTDOANHNGHIEP.THONGTINDN)
        </dd>

        <dt>
            Địa Chỉ Doanh Nghiệp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TTDOANHNGHIEP.DIACHI)
        </dd>
    </dl>
</div>
</div>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="Xóa" class="btn btn-default" /> |
        <a href="/Admin/TaiKhoanDN">&lt&lt Trở lại</a>
    </div>
}
